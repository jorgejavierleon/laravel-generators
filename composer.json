{
    "name": "jorgejavierleon/generator",
    "description": "Laravel classes generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Javier Leon",
            "email": "jorgejavierleon@gmail.com",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/jorgejavierleon/generator",
    "keywords": ["Laravel", "Generator"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Jorgejavierleon\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jorgejavierleon\\Generator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jorgejavierleon\\Generator\\GeneratorServiceProvider"
            ],
            "aliases": {
                "Generator": "Jorgejavierleon\\Generator\\Facades\\Generator"
            }
        }
    }
}
